{
	"schemaVersion": 1,
	"id": "reverted_backend",
	"version": "${version}",
	"name": "Reverted Backend",
	"description": "\"Reverts\" stuff like new Identifier() needing to be Identifier.of(), adds back nbt to items, and more!",
	"authors": [
		"Mavity The Madity"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"icon": "assets/reverted_backend/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"wiiu.mavity.reverted_backend.RevertedBackend"
		]
	},
	"mixins": [
		"reverted_backend.mixins.json"
	],
	"accessWidener": "reverted_backend.accesswidener",
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	},
	"custom": {
		"loom:injected_interfaces": {
			"net/minecraft/class_1799": ["wiiu/mavity/reverted_backend/interfaces/INbtInjection"]
		}
	}
}